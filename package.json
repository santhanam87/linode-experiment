{
  "name": "ginger-bread",
  "version": "0.0.0",
  "nodemonConfig": {
    "exec": "yarn dev",
    "watch": [
      "src/server/*",
      "src/client/*"
    ]
  },
  "type": "module",
  "scripts": {
    "start": "node ./src/server/bin/www",
    "watch": "node --inspect dist/server.cjs",
    "build": "webpack --config webpack.config.js",
    "dev": "npm-run-all build watch",
    "build:module": "webpack --config es6.webpack.config.js",
    "watch:dev": "nodemon"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@reduxjs/toolkit": "^1.7.1",
    "apollo-server": "^3.6.3",
    "apollo-server-core": "^3.6.3",
    "apollo-server-express": "^3.6.3",
    "cookie-parser": "~1.4.4",
    "core-js": "^3.21.0",
    "debug": "^4.3.3",
    "express": "~4.16.1",
    "graphql": "^16.3.0",
    "morgan": "~1.9.1",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.8.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.17.4",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@babel/register": "^7.17.0",
    "babel-loader": "^8.2.3",
    "eslint": "^8.6.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "html-webpack-plugin": "^5.5.0",
    "nodemon": "^2.0.15",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-node-externals": "^3.0.0"
  }
}
